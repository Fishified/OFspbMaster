FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
	upstream
	{ 
        	type            zeroGradient;
		//value		101.35;
	}

	downstream
    	{
        type            fixedValue;
        value           uniform 0;        	
    	}

	PipeUpDown
	{ 
        	type            zeroGradient;
	}

	PipeBaffle
	{ 
        	type            zeroGradient;
	}

	baffles
	{ 
        	type            zeroGradient;
	}

	wall
	{ 
        	type            zeroGradient;
	}

}

